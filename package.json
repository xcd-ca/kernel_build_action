{
  "name": "kernel-build-action",
  "version": "1.0.0",
  "description": "Android Kernel Build Action",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist && ncc build src/post.ts -o dist/post",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts"
  },
  "dependencies": {
    "@actions/artifact": "^2.1.0",
    "@actions/cache": "^3.2.4",
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/tool-cache": "^2.0.1",
    "@octokit/rest": "^22.0.1"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}
